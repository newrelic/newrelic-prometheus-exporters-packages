{
  "config_protocol_version": "1",
  "action": "register_config",
  "config_name": "cfg-{{.integration}}",
  "config": {
    "variables": {},
    "integrations": [
         {
            "name": "nri-prometheus",
            {{set "interval" .env.interval}}
            "config": {
                "standalone": false,
                {{set "verbose" .env.VERBOSE}}
                {{.entity_definitions}},
                "targets": [
                    {
                        "urls": [
                            "http://localhost:{{.exporter_port}}"
                        ]
                    }
                ]

            }
        },
        {{.exporter_definition}}
    ]
  }
}
